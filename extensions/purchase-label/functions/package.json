{
  "name": "shipengine-purchase-label-functions",
  "scripts": {
    "prepare": "npm run build",
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "emulate": "yarn build && firebase ext:dev:emulators:start --test-params ../.env.development --project shipengine-firebase-extensions",
    "test": "yarn build && firebase ext:dev:emulators:exec --test-params ../.env.test --project=shipengine-firebase-extensions 'yarn run-test'",
    "run-test": "mocha --timeout=10000 'lib/test/**/*.spec.js' --exit"
  },
  "engines": {
    "node": "15"
  },
  "main": "lib/index.js",
  "dependencies": {
    "chai": "^4.3.4",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "mocha": "^9.1.1",
    "shipengine": "^1.0.0",
    "shipengine-firebase-common": "^1.0.0",
    "typescript": "^3.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/humps": "^2.0.1",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
